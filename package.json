{
  "name": "llms-txt-generator",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "next build",
    "vercel-build": "prisma generate && prisma db push --accept-data-loss && next build",
    "check": "biome check .",
    "lint": "biome check --write --unsafe .",
    "check:write": "biome check --write .",
    "db:generate": "prisma migrate dev",
    "db:migrate": "prisma migrate deploy",
    "db:push": "prisma db push",
    "db:studio": "prisma studio",
    "dev": "next dev --turbo",
    "postinstall": "prisma generate",
    "preview": "next build && next start",
    "start": "next start",
    "typecheck": "tsc --noEmit",
    "precommit": "pnpm run typecheck && pnpm run lint",
    "test:integration": "node test/integration-tests/simple-test.js",
    "test:crawl:mock": "tsx test/integration-tests/crawl-pipeline.mock.test.ts",
    "test:crawl": "tsx test/integration-tests/crawl-pipeline.test.ts",
    "inngest:dev": "pnpm dlx inngest-cli@latest dev --no-poll"
  },
  "dependencies": {
    "@prisma/client": "^6.5.0",
    "@supabase/supabase-js": "^2.58.0",
    "@t3-oss/env-nextjs": "^0.12.0",
    "@types/diff": "^8.0.0",
    "diff": "^8.0.2",
    "firecrawl": "^4.3.6",
    "inngest": "^3.44.0",
    "next": "^15.2.3",
    "openai": "^6.0.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "zod": "^3.24.2"
  },
  "devDependencies": {
    "@biomejs/biome": "1.9.4",
    "@tailwindcss/postcss": "^4.0.15",
    "@types/node": "^20.14.10",
    "@types/react": "^19.0.0",
    "@types/react-dom": "^19.0.0",
    "dotenv": "^17.2.3",
    "postcss": "^8.5.3",
    "prisma": "^6.5.0",
    "tailwindcss": "^4.0.15",
    "typescript": "^5.8.2"
  },
  "ct3aMetadata": {
    "initVersion": "7.39.3"
  },
  "packageManager": "pnpm@10.11.0"
}
